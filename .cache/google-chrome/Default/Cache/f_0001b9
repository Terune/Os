    <script type="text/javascript">
		$(function() {
			// 왼쪽 메뉴 tooltip
			$("#page-ubstrap-side ul.left_menu a").tooltip({placement:'right'});

			// sections scroll 적용
			$("#page-content #region-pre .course_sections .section_scroll, #page-content #region-pre .activities .section_scroll").mCustomScrollbar({
				theme:"ubstrap",
				scrollInertia:500
			});


			
			// activity 숨김/표시
			$("#page-content #region-pre .section_scoll_showhide").click(function() {
				image_src = $(this).attr('src');
				li = $(this).parent().parent().parent();
				cookiename = li.attr('data-name');

				if(image_src.indexOf('switch_minus') >= 0) {
					image_src = image_src.replace("switch_minus", "switch_plus");
					$.cookie('coursemos-' + cookiename, 'hide', { path:'/' });

					//$(this).parent().parent().parent().find('.section_scroll').hide();
					li.addClass('hide_section_scroll');
				} else {
					image_src = image_src.replace("switch_plus", "switch_minus");
					$.cookie('coursemos-' + cookiename, 'show', { path:'/' });

					//$(this).parent().parent().parent().find('.section_scroll').show();
					li.removeClass('hide_section_scroll');
				}

				$(this).attr('src', image_src);

				// 주차, 학습활동 열고 닫을때마다 scroll도 update되어야 함.
				$("#page-content #region-pre .course_sections .section_scroll, #page-content #region-pre .activities .section_scroll").mCustomScrollbar('update');

				return false;
			});

			// activity 메뉴 자체를 클릭했을 때에도 section_scroll_showhide 효과내기
			$("#page-content #region-pre .course_sections .activities .title").click(function() {
				$("#page-content #region-pre .course_sections .activities .section_scoll_showhide").trigger('click');
				return false;
			});
		});
    </script>
